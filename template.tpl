const injectScript = require('injectScript');
const callInWindow = require('callInWindow');
const log = require('logToConsole');
const set = require('setInWindow');
const createArgumentsQueue = require('createArgumentsQueue');


let modio = function () {
  createArgumentsQueue('modio', 'modio.q');
};

set('modio', modio);

const onSuccess = function () {
    
  callInWindow('modio', 'init', data.shop_id);
    
  if (data.action == 'purchase') {        
    callInWindow('modio', 'purchase', { 
      order_id : data.order_id, 
      price : data.price, 
      currency: data.currency, 
      items : data.items
    });
  }
  
  data.gtmOnSuccess();
};

const onFailure = function () {
  log('Chyba pri nacitani Modio pixelu'); 
  data.gtmOnFailure();
};

injectScript('https://trackingapi.modio.cz/ppc.js', onSuccess, onFailure);


